    "ava": "^6.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "mock-req-res": "^1.2.1",
    "mongodb-memory-server": "^9.1.6",
    "nock": "^13.5.4",
    "nodemon": "^3.0.2",
    "nyc": "^15.1.0",
    "sinon": "^17.0.1",
    "sonarqube-scanner": "^3.3.0",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "common-errors": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "got": "^14.2.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
